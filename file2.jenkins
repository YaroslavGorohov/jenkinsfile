pipeline {
  triggers {
    githubPush()
  }
  agent {
    label 'master'
  }
  options {
    disableConcurrentBuilds()
    buildDiscarder (logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
    timestamps()
  }
  stages {
         stage('Linux') {
					    agent { label 'net-core' }
					      steps {
                sh 'hostname'
                sh 'pwd'
         
      }
    }
  }
}
