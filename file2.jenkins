pipeline {
  triggers {
    githubPush()
  }
    options {
    disableConcurrentBuilds()
    buildDiscarder (logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
     }
  stages {
         stage('Linux') {
		agent { label 'net-core' }
			steps {
                sh 'hostname'
                sh 'pwd'
         
      }
    }
  }
}
