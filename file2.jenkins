pipeline {
  triggers {
    githubPush()
  }
  agent {
    label 'ubuntu'
  }
  options {
    disableConcurrentBuilds()
    buildDiscarder (logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
   # timestamps()
  }
  stages {
    stage("First step") {
      steps {
        sh 'hostname'
        sh 'pwd'
      }
    }
  }
}         
