pipeline {
  triggers {
    githubPush()
  }
  agent {
    label 'net-core'
  }
  options {
    disableConcurrentBuilds()
    buildDiscarder (logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
  }
  stages {
  stage("First step") {
      agent {
        label 'net-core'
      }
      steps {
        sh 'hostname'
        sh 'pwd'
      }
    }
  }
} 
